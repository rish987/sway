# terminal emulator
set $term lxterminal
# internet browser
set $browser chromium
set $browser_datadir /home/rish/snap/chromium/common/chromium/
set $filetree pcmanfm
# scratchpad size
set $scratchw 1850
set $scratchh 1020
# scratchpad media dir
set $scratchdir ~/Downloads/youtube

# WAYLAND_DISPLAY env variable value for primary/nested
set $waydisp wayland-0
set $waydispnest wayland-1
